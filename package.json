{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "description": "This is a server for the variety popup shop",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require test/setup.js --watch",
    "start": "node ./src/server",
    "dev": "nodemon ./src/server",
    "predeploy": "npm audit && npm run migrate:production",
    "deploy": "git push heroku master",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "migrate:production": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romiaujla/express-boilerplate.git"
  },
  "keywords": [],
  "engines": {
    "node": "10.16.0"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/romiaujla/express-boilerplate/issues"
  },
  "homepage": "https://github.com/romiaujla/express-boilerplate#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.18.2",
    "helmet": "^3.21.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.4",
    "morgan": "^1.9.1",
    "pg": "^7.14.0",
    "treeize": "^2.1.2",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.2",
    "postgrator-cli": "^3.3.0",
    "supertest": "^4.0.2"
  }
}
